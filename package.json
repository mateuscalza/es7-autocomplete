{
  "name": "es7-autocomplete",
  "version": "1.0.4",
  "description": "Pure JS, with ES7 features, without jQuery, object-oriented.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run connect\" \"webpack -w\"",
    "build": "webpack",
    "connect": "browser-sync start --dev --server --index='example/index.html' --files='example/*.*,dist/index.js'",
    "test": "karma start karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateuscalza/es7-autocomplete.git"
  },
  "keywords": [
    "autocomplete",
    "es7",
    "es6",
    "pure",
    "javascript",
    "without",
    "jquery",
    "multi",
    "custom",
    "json",
    "set",
    "other",
    "fields",
    "additional",
    "data"
  ],
  "author": "Inovadora Sistemas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mateuscalza/es7-autocomplete/issues"
  },
  "homepage": "https://github.com/mateuscalza/es7-autocomplete#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-regenerator-runtime": "^6.5.0",
    "browser-sync": "^2.13.0",
    "concurrently": "^2.1.0",
    "karma": "^1.1.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-should": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-verbose-reporter": "0.0.3",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "should": "^9.0.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "extend": "^3.0.0"
  }
